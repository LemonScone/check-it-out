{
  "name": "check-it-out",
  "version": "0.1.0",
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.checkitout.app",
    "productName": "Check It Out",
    "mac": {
      "icon": "public/assets/icons/mac/icon.png"
    },
    "win": {
      "icon": "public/assets/icons/win/icon.png"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.1",
    "@mui/x-data-grid": "^4.0.1",
    "active-win": "^7.7.1",
    "date-fns": "^2.28.0",
    "electron-store": "^8.0.1",
    "extract-file-icon": "^0.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-id-generator": "^3.0.2",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "electron": "^14.0.0",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "wait-on": "^6.0.0"
  },
  "scripts": {
    "start": "concurrently \"BROWSER=none npm run react:start\" \"npm run electron:start\"",
    "react:start": "react-scripts start",
    "electron:start": "wait-on http://localhost:3000 && mode=dev electron .",
    "react:build": "react-scripts build",
    "electron:build": "electron-builder -mw",
    "build": "yarn react:build && yarn electron:build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
